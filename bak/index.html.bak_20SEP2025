<script>
  (function(){
    const ipEl = document.getElementById('client-ip');
    const uaEl = document.getElementById('user-agent');
    if (uaEl) uaEl.value = navigator.userAgent || '';

    async function setIP(){
      if (!ipEl) return;
      if (ipEl.value) return; // already set
      try {
        // Prefer IPv6-capable endpoint; falls back if blocked
        const ctrl = new AbortController();
        const t = setTimeout(()=>ctrl.abort(), 4000);
        let res = await fetch('https://api64.ipify.org?format=json', { signal: ctrl.signal });
        clearTimeout(t);
        if (!res.ok) throw new Error('ipify failed');
        const data = await res.json();
        if (data && data.ip) ipEl.value = data.ip;
      } catch(e){
        try {
          const r = await fetch('https://api.ipify.org?format=json');
          const d = await r.json();
          if (d && d.ip) ipEl.value = d.ip;
        } catch(_) {
          // last-resort fallback endpoint
          try {
            const r2 = await fetch('https://api.my-ip.io/ip.json');
            const d2 = await r2.json();
            if (d2 && (d2.ip || d2.address)) ipEl.value = d2.ip || d2.address;
          } catch(_) { /* give up silently */ }
        }
      }
    }

    // Try on load
    setIP();

    // Ensure IP is present right before AJAX submission too
    const form = document.getElementById('contact-form');
    if (form) {
      form.addEventListener('submit', function(){ setIP(); }, { capture:true });
    }
  })();
</script>

  </section>
