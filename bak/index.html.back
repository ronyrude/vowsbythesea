<script>
  // Toggle read more/less in packages
  document.querySelectorAll('.package .readmore').forEach(btn => {
    btn.addEventListener('click', () => {
      const packageEl = btn.closest('.package');
      const desc = packageEl.querySelector('.desc');
      const expanded = packageEl.classList.toggle('expanded');
      desc.hidden = !expanded;
      btn.setAttribute('aria-expanded', expanded);
      packageEl.setAttribute('aria-expanded', expanded);
      btn.textContent = expanded ? 'READ LESS' : 'READ MORE';
    });
    btn.addEventListener('keydown', (e) => {
      if(e.key === 'Enter' || e.key === ' ') {
        e.preventDefault();
        btn.click();
      }
    });
  });

  // Simple Contact Form validation and fake submission
  const form = document.getElementById('contact-form');
  const messageDiv = document.getElementById('form-message');
  form.addEventListener('submit', (e) => {
    e.preventDefault();
    messageDiv.textContent = '';
    if(!form.checkValidity()) {
      messageDiv.style.color = 'red';
      messageDiv.textContent = 'Please fill out all required fields correctly.';
      form.reportValidity();
      return;
    }

    // Simulate sending form (replace with real API / backend)
    messageDiv.style.color = 'green';
    messageDiv.textContent = 'Thank you! Your message has been sent.';
    form.reset();
  });
</script>

