<style>
  :root {
    --primary: #f28e1c;
    --dark: #1a1a1a;
    --light: #fff;
    --text-dark: #222;
    --text-muted: #666;
    --border-light: #ddd;
    --radius: 10px;
    --max-width: 1000px;
    --font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  }

  * {
    box-sizing: border-box;
  }

  body {
    margin: 0;
    font-family: var(--font-family);
    background: var(--light);
    color: var(--text-dark);
    line-height: 1.5;
    text-align: center;
    min-height: 100vh;
    padding-bottom: 60px;
  }

  nav {
    background: var(--dark);
    padding: 12px 0;
    position: sticky;
    top: 0;
    z-index: 1000;
    box-shadow: 0 2px 8px rgba(0,0,0,0.3);
  }
  nav a {
    color: var(--light);
    text-decoration: none;
    margin: 0 20px;
    font-weight: 600;
    font-size: 1.1rem;
    transition: color 0.3s;
  }
  nav a:hover,
  nav a.active {
    color: var(--primary);
  }

  header {
    background: linear-gradient(rgba(0,0,0,0.45), rgba(0,0,0,0.45)), url('https://cdn.pixabay.com/photo/2016/02/19/11/53/clearwater-beach-1209438_1280.jpg') center/cover no-repeat;
    padding: 100px 20px 120px;
    color: var(--light);
    user-select: none;
  }
  header h1 {
    font-size: 3.5rem;
    margin-bottom: 0.2em;
  }
  header p {
    font-size: 1.3rem;
    max-width: 650px;
    margin: 0 auto 30px;
  }
  .btn {
    background: var(--primary);
    color: var(--light);
    padding: 15px 40px;
    font-weight: 700;
    border: none;
    border-radius: var(--radius);
    cursor: pointer;
    text-decoration: none;
    display: inline-block;
    margin: 0 10px;
    box-shadow: 0 5px 10px rgba(242, 142, 28, 0.5);
    transition: background-color 0.3s;
  }
  .btn:hover,
  .btn:focus {
    background: #d87600;
    outline: none;
  }

  main {
    max-width: var(--max-width);
    margin: 50px auto 0;
    padding: 0 20px;
  }

  h2 {
    font-size: 2.5rem;
    color: var(--primary);
    margin-bottom: 30px;
    user-select: none;
  }

  /* Packages */
  .packages {
    display: grid;
    grid-template-columns: repeat(auto-fit,minmax(320px,1fr));
    gap: 25px;
    margin-bottom: 70px;
  }
  .package {
    border: 1px solid var(--border-light);
    border-radius: var(--radius);
    padding: 15px 20px 10px;
    text-align: left;
    background: var(--light);
    box-shadow: 0 4px 14px rgba(0,0,0,0.08);
    cursor: default;
    user-select: none;
    transition: box-shadow 0.3s;
    position: relative;
  }
  .package:hover,
  .package:focus-within {
    box-shadow: 0 10px 30px rgba(242, 142, 28, 0.5);
    outline: none;
  }
  .package h3 {
    margin-top: 0;
    margin-bottom: 6px;
    font-weight: 900;
    color: var(--dark);
    font-size: 1.5rem;
  }
  .package .price {
    color: var(--primary);
    font-weight: 700;
    font-size: 1.3rem;
    margin-bottom: 16px;
  }
  .package p.desc {
    font-weight: 600;
    color: var(--text-dark);
    margin-bottom: 12px;
    white-space: normal;
    max-height: 100px;
    overflow: hidden;
    transition: max-height 0.5s ease;
  }
  .package.expanded .desc {
    max-height: 1000px;
  }
  .package .readmore {
    color: var(--primary);
    font-weight: 700;
    font-size: 0.95rem;
    cursor: pointer;
    user-select: none;
    display: inline-block;
  }
  .package .readmore:hover,
  .package .readmore:focus {
    text-decoration: underline;
    outline: none;
  }

  /* Package images */
  .package img {
    width: 100%;
    border-radius: var(--radius);
    margin-bottom: 10px;
    object-fit: cover;
    max-height: 180px;
  }

  /* Beaches */
  .beaches {
    max-width: var(--max-width);
    margin: 0 auto 80px;
  }
  .beach-list {
    display: grid;
    grid-template-columns: repeat(auto-fit,minmax(280px,1fr));
    gap: 25px;
  }
  .beach-card {
    border-radius: var(--radius);
    overflow: hidden;
    box-shadow: 0 6px 20px rgba(0,0,0,0.1);
    cursor: pointer;
    background: var(--light);
    display: flex;
    flex-direction: column;
    user-select: none;
    transition: box-shadow 0.3s ease;
  }
  .beach-card:hover,
  .beach-card:focus-within {
    box-shadow: 0 12px 36px rgba(242, 142, 28, 0.5);
    outline: none;
  }
  .beach-card img {
    width: 100%;
    height: 180px;
    object-fit: cover;
    flex-shrink: 0;
  }
  .beach-card a {
    padding: 15px 10px;
    font-weight: 700;
    font-size: 1.1rem;
    color: var(--primary);
    text-decoration: none;
    flex-grow: 1;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .beach-card a:hover,
  .beach-card a:focus {
    color: #d87600;
    outline: none;
    text-decoration: underline;
  }

  /* Contact form */
  #contact {
    max-width: var(--max-width);
    margin: 0 auto 80px;
    padding: 20px;
    background: #f9f9f9;
    border-radius: var(--radius);
    box-shadow: 0 5px 15px rgba(0,0,0,0.05);
    text-align: left;
  }
  #contact h2 {
    color: var(--primary);
    text-align: center;
    margin-bottom: 30px;
  }
  form {
    display: flex;
    flex-direction: column;
    gap: 16px;
  }
  label {
    font-weight: 600;
    display: block;
  }
  input, textarea {
    padding: 10px;
    border-radius: var(--radius);
    border: 1px solid var(--border-light);
    font-size: 1rem;
    width: 100%;
    resize: vertical;
  }
  input:focus, textarea:focus {
    outline: 2px solid var(--primary);
  }
  button.submit-btn {
    background: var(--primary);
    color: var(--light);
    padding: 15px;
    border: none;
    font-weight: 700;
    cursor: pointer;
    border-radius: var(--radius);
    transition: background-color 0.3s ease;
  }
  button.submit-btn:hover {
    background: #d87600;
  }
  .form-message {
    font-weight: 600;
    font-size: 1rem;
    margin-top: 10px;
    color: green;
  }

  /* Responsive typography */
  @media (max-width: 900px) {
    header h1 {
      font-size: 2.8rem;
    }
    h2 {
      font-size: 2rem;
    }
  }

  @media (max-width: 600px) {
    header h1 {
      font-size: 2.4rem;
    }
    .packages {
      grid-template-columns: 1fr;
    }
    .beach-list {
      grid-template-columns: 1fr;
    }
  }
</style>
</head>
